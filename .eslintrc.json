{
    "parserOptions": {
      "ecmaVersion": 2018,
      "sourceType": "module"
    },
    "extends": [
        "plugin:vue/base",
        "eslint:recommended",
        "@vue/typescript"
    ],
    "rules": {
        "indent": ["error", 4]
    },
    "globals": {
        "document": false,
        "console": false,
        "require": false,
        "setTimeout": false,
        "clearTimeout": false
    }
  }