{
    "parserOptions": {
      "ecmaVersion": 6,
      "sourceType": "module"
    },
    "env": {
      "es6": true
    },
    "extends": [
        "htmlacademy/es6",
        "plugin:vue/essential"
    ],
    "rules": {
        "indent": ["error", 4]
    },
    "globals": {
        "document": false,
        "console": false,
        "require": false,
        "setTimeout": false,
        "clearTimeout": false
    }
  }